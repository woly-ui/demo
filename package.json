{
  "name": "demo",
  "private": true,
  "description": "Demo page based on woly-ui components",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "start": "razzle start --type=spa",
    "start:prod": "serve -s build/public",
    "build:demo": "razzle build --type=spa",
    "test": "razzle test --env=jsdom --passWithNoTests",
    "build": "rollup -c",
    "format": "prettier --check '{src,build,.storybook,packages}/**/**.{ts,tsx,js,md,mdx,json}'",
    "format:fix": "prettier --write '{src,build,.storybook,packages}/**/**.{ts,tsx,js,md,mdx,json}'",
    "lint:code": "eslint --ext .ts,.tsx src",
    "lint:code:fix": "yarn lint:code --fix",
    "lint:style": "stylelint './src/**/*.{js,css,ts,tsx}'"
  },
  "dependencies": {
    "reflect-form": "^0.1.2"
  },
  "peerDependencies": {
    "effector": "^21.8.0",
    "react": "^16.14.0 || ^17.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@effector/reflect": "^0.6.0",
    "@eslint-kit/eslint-config-base": "^3.0.0",
    "@eslint-kit/eslint-config-node": "^2.0.0",
    "@eslint-kit/eslint-config-prettier": "^2.0.0",
    "@eslint-kit/eslint-config-react": "^2.1.0",
    "@eslint-kit/eslint-config-typescript": "^3.2.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^18.1.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.7",
    "@types/styled-components": "^5.1.7",
    "@types/webpack-env": "^1.16.0",
    "core-js": "^3.8.2",
    "effector": "^21.8.11",
    "effector-react": "^21.3.2",
    "eslint": "^7.20.0",
    "express": "^4.17.1",
    "patronum": "^1.1.0",
    "prettier": "2.2.1",
    "prettier-plugin-import-sort": "^0.0.6",
    "razzle": "^3.3.9",
    "razzle-plugin-typescript": "^3.3.9",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-is": "^16.8.0",
    "react-router-dom": "^5.2.0",
    "rollup": "^2.47.0",
    "rollup-plugin-dts": "^3.0.1",
    "rollup-plugin-terser": "^7.0.2",
    "styled-components": "^5.2.1",
    "stylelint": "^13.10.0",
    "stylelint-config-idiomatic-order": "^8.1.0",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-no-unsupported-browser-features": "^4.1.4",
    "stylelint-order": "^4.1.0",
    "stylelint-prettier": "^1.1.2",
    "tslint": "^6.1.3",
    "typescript": "^4.1.3",
    "webpack": "^4.0.0",
    "webpack-cli": "^4.6.0",
    "woly": "^0.10.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/woly-ui/demo"
  }
